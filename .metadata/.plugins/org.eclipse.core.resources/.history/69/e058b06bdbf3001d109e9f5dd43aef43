import java.util.Scanner;

public class Enunciado01 {
	
	String[] nomes = new String [10]; 
	Scanner objScanner = new Scanner(System.in);
	
	public void listarNomes() {
		
		System.out.println("ADICIONE O PRIMEIRO NOME ");
		System.out.println("ADICIONE O SEGUNDO NOME ");
		System.out.println("ADICIONE O TERCEIRO NOME ");
		System.out.println("ADICIONE O QUARTO NOME ");
		System.out.println("ADICIONE O QUINNTO NOME ");
		System.out.println("ADICIONE O SEXTO NOME ");
		System.out.println("ADICIONE O SÉTIMO NOME ");
		System.out.println("ADICIONE O OITAVO NOME ");
		System.out.println("ADICIONE O NONO NOME ");
		System.out.println("ADICIONE O DÉCIMO NOME ");
		
		for(int indice = 0; indice < nomes.length; indice++); {
			System.out.println("LISTAGEM DE NOMES: ");
		}
	}
}

